{
  "name": "mui-mdx-components",
  "version": "0.5.0",
  "description": "MDX components for material UI",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && cp package.json README.md LICENSE ./dist",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "example": "vite ./example --config vite.config.ts --host",
    "example-preview": "vite preview ./example --config vite.config.ts --host",
    "example-build": "npm run clean && tsc && vite build ./example --config vite.config.ts "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joesturge/mui-mdx-components.git"
  },
  "keywords": [
    "mdx",
    "mui",
    "markdown",
    "material",
    "ui",
    "components"
  ],
  "author": "Joe Sturgeon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joesturge/mui-mdx-components/issues"
  },
  "homepage": "https://github.com/joesturge/mui-mdx-components#readme",
  "peerDependencies": {
    "@mdx-js/mdx": ">=2.0.0",
    "@mui/material": ">=5.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "react-syntax-highlighter": ">=15.0.0"
  },
  "peerDependenciesMeta": {
    "react-syntax-highlighter": {
      "optional": true
    }
  },
  "devDependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mdx-js/react": "2.3.0",
    "@mdx-js/rollup": "2.3.0",
    "@mui/material": "5.14.12",
    "@testing-library/react": "12.1.5",
    "@types/jest": "29.5.5",
    "@types/node": "20.8.2",
    "@types/react": "17.0.67",
    "@types/react-dom": "17.0.21",
    "@types/react-syntax-highlighter": "15.5.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react": "4.1.0",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.51.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "happy-dom": "10.11.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-syntax-highlighter": "15.5.0",
    "typescript": "5.2.2",
    "vite": "4.4.11",
    "vitest": "0.34.6"
  },
  "optionalDevDependencies": {
    "@esbuild/android-arm": "0.18.4",
    "fsevents": "2.3.2"
  }
}
